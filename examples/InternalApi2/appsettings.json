{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:8120",
        "Protocols": "Http1"
      },
      "gRPC": {
        "Url": "http://*:8121",
        "Protocols": "Http2"
      }
    }
  },
  "http": {
    "public-api": "http://public-api:8100",
    "internal-api1": "http://internal-api1:8110",
    "internal-api2": "http://internal-api2:8120"
  },
  "grpc": {
    "public-api": "http://public-api:8101",
    "internal-api1": "http://internal-api1:8111",
    "internal-api2": "http://internal-api2:8121"
  },
  "aws-sqs": {
    "public-api": "http://localstack:4566/000000000000/public-api.fifo",
    "internal-api2": "http://localstack:4566/000000000000/internal-api2.fifo"
  },
  "redis": {
    "public-api": "redis:6379",
    "internal-api2": "redis:6379"
  }
}
